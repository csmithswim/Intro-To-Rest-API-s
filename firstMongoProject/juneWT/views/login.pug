html
    head
        style
            include ../public/login.css

        script
            include ../public/login.js

        title Login
        body
            #container
                h1#loginHead Login
                #form
                    label.lables Email
                    input#email(minLength=5 type='email' placeholder='Enter Email')
                    label.lables Password
                    input#password(minLength=7 type='password' placeholder='Enter Password')
                button#submitLogin Login
